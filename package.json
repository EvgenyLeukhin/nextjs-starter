  },
  "bugs": "https://github.com/EvgenyLeukhin/nextjs-starter/issues",
  "homepage": "https://EvgenyLeukhin.github.io/nextjs-starter",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "serve": "node ./utils/server.js",
    "analyze": "ANALYZE=true next build",
    "analyze:server": "BUNDLE_ANALYZE=server next build",
    "analyze:browser": "BUNDLE_ANALYZE=browser next build",
    "deploy:gh-pages": "next build && gh-pages -d .next",
    "deploy:netlify": "git checkout netlify && git merge main && git push origin netlify"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "next": "13.1.6",
    "normalize.css": "^8.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.1.6",
    "@next/font": "13.1.6",
    "@types/node": "18.11.18",
    "@types/prettier": "^2.7.2",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "compression": "^1.7.4",
    "compression-webpack-plugin": "^10.0.0",
    "console-log-colors": "^0.3.5",
    "eslint": "8.33.0",
    "eslint-config-next": "13.1.6",
    "express": "^4.18.2",
    "gh-pages": "^5.0.0",
    "prettier": "^2.8.3",
    "sass": "^1.58.0",
    "typescript": "4.9.5"
  },
  "engines": {
    "node": ">=16.14.0",
    "npm": ">=9.2.0"
  }
}

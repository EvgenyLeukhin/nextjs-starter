{
  "name": "nextjs-starter",
  "version": "0.1.0",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/EvgenyLeukhin/nextjs-starter.git"
  },
  "keywords": [
    "nextjs",
    "react",
    "starter"
  ],
  "author": {
    "name": "Evgeny Leukhin",
    "email": "EvgenyLeukhin@ya.ru",
    "url": "https://vk.com/leukhin_ei"
  },
  "bugs": "https://github.com/EvgenyLeukhin/nextjs-starter/issues",
  "homepage": "https://EvgenyLeukhin.github.io/nextjs-starter",
  "scripts": {
    "dev": "cross-env NODE_ENV=development env-cmd -f ./api/.env.development next dev",
    "build:test": "cross-env NODE_ENV=production env-cmd -f ./api/.env.development next build",
    "build:prod": "cross-env NODE_ENV=production env-cmd -f ./api/.env.production next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "serve": "cross-env NODE_ENV=production node ./utils/server.js",
    "analyze": "ANALYZE=true next build",
    "analyze:server": "BUNDLE_ANALYZE=server next build",
    "analyze:browser": "BUNDLE_ANALYZE=browser next build",
    "deploy": "git checkout netlify && git merge main && git push origin netlify"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "classnames": "^2.3.2",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "next": "13.1.6",
    "normalize.css": "^8.0.1",
    "react": "18.2.0",
    "react-awesome-reveal": "^4.2.3",
    "react-device-detect": "^2.2.3",
    "react-dom": "18.2.0",
    "react-share": "^4.4.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.1.6",
    "@next/font": "13.1.6",
    "@types/node": "18.11.18",
    "@types/prettier": "^2.7.2",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "compression": "^1.7.4",
    "compression-webpack-plugin": "^10.0.0",
    "console-log-colors": "^0.3.5",
    "eslint": "8.33.0",
    "eslint-config-next": "13.1.6",
    "express": "^4.18.2",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3",
    "sass": "^1.58.0",
    "typescript": "4.9.5"
  },
  "engines": {
    "node": ">=16.14.0",
    "npm": ">=9.2.0"
  }
}
